{
	"name": "simple-scrum",
	"version": "1.0.0",
	"description": "Just a simple scrum board",
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"test": "jest",
		"test:ci": "jest --ci",
		"generate": "generate",
		"generate:svg": "yarn svgr ./svgs/ --out-dir ./svgs/@generated --ignore-existing --typescript --memo",
		"format": "prettier --write .",
		"lint:eslint": "next lint",
		"lint:typescript": "tsc --noEmit",
		"lint": "yarn lint:typescript && yarn lint:eslint",
		"release": "release-it",
		"prepare": "husky install"
	},
	"repository": "https://github.com/remcolakens/simple-scrum",
	"author": "Remco Lakens",
	"license": "MIT",
	"devDependencies": {
		"@remcolakens/simple-component-generator": "^1.0.4",
		"@svgr/cli": "^6.5.1",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^14.4.3",
		"@types/node": "^18.11.9",
		"@types/react": "^18.0.25",
		"@types/react-dom": "^18.0.9",
		"@typescript-eslint/eslint-plugin": "^5.44.0",
		"@typescript-eslint/parser": "^5.44.0",
		"babel-jest": "^29.3.1",
		"eslint": "^8.28.0",
		"eslint-config-next": "^13.0.4",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-testing-library": "^5.9.1",
		"husky": "^8.0.2",
		"jest": "^29.3.1",
		"jest-environment-jsdom": "^29.3.1",
		"lint-staged": "^13.0.3",
		"prettier": "^2.8.0",
		"prettier-plugin-organize-imports": "^3.2.0",
		"release-it": "^15.5.0",
		"typescript": "^4.9.3"
	},
	"dependencies": {
		"@chakra-ui/icons": "^2.0.12",
		"@chakra-ui/react": "^2.4.1",
		"@emotion/react": "^11.10.5",
		"@emotion/styled": "^11.10.5",
		"@fontsource/open-sans": "^4.5.13",
		"@types/react-beautiful-dnd": "^13.1.2",
		"framer-motion": "^7.6.9",
		"next": "^13.0.4",
		"react": "^18.2.0",
		"react-beautiful-dnd": "^13.1.1",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.39.5",
		"uuid": "^9.0.0"
